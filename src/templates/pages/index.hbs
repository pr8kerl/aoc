---
title: Add-On Centre
lead: Find add-on solutions to extend and build on your existing business management and accounting software.
---

<div class="box padded">

<div class="wrapper">
<!-- MAIN BODY HERE -->

<!-- #BEGIN Addon Page Body Content -->
<div class="row head_message">
    <!-- put head titles etc here -->
    
        <h1>{{ title }}</h1>
        <p>{{ lead }}</p>
        <hr>
</div>
<!-- end head message -->
<script type="text/JavaScript">
$(document).ready(function(){
    var categories = 'down';
    var industry = 'up';
    // add for categories
    $('#categories').click(function(){
        if(categories == 'up') {
            $('#cat_toggle').src = '/addons/static/img/Down_24x24.png';
            categories = 'down';
        } else {
            $('#cat_toggle').src = '/addons/static/img/Up_24x24.png';
            categories = 'up';
        }
    })
});
</script>
 <div class="row-fluid">
        {{> main-sidebar }}

        <div class="span8">

    
      <div class="row-fluid image-listings">
         <script type="text/javascript">
        $(document).ready(function(){
            $('.worksWith img').mouseenter(function(){
                $(this).tooltip('show');
            }).mouseleave(function(){
                $(this).tooltip('hide');
            });
        });

        
        </script>

          
              <div class="featured_group">

              {{#each listings}}
                {{#if this.is_promoted}}
                  <div class="featured_tile">
										<a href="{{ this.url }}/">
                    <div class="featured_logo pull-left">
                      <img class="tile-image big-illustration" alt="" width="110" height="110" src="/addons{{ this.images.logo }}">
                    </div>
                    </a>
										<div class="featured_content pull-left">
											<a href="{{ this.url }}/">
                      <h3 class="tile-title">{{ this.name }}</h3>
                          <p>{{ this.seo.description }}</p>
                      </a>
											<div class="worksWith featured_worksWith"><a href="{{ this.url }}/"></a>

                                {{#each this.workswith}}
                                <a href="/addons/workswith/fix-me/">
                                    <img src="/addons{{ this.icon }}" alt="{{ this.name }}" data-toggle="tooltip" title="{{ this.name }}">
                                </a>
																{{/each}}

                          </div>
                        </div>
                    </div>
                    {{/if}}
                  {{/each}}
                  
                
            </div>
          

          
            <div class="listing_break"><hr></div>
          

          
            <div class="ppartner_listings">

              {{#each listings}}
                {{#compare this.developer.level 'PP' }}
              
              <!-- paid -->
                <a href="{{ this.url }}/">
                  </a><div><a href="{{ this.url }}/">
                    </a><div class="tile"><a href="{{ this.url }}/">
                      <img class="tile-image big-illustration" alt="" src="/addons{{ this.images.logo }}">
                      <h3 class="tile-title">{{ this.name }}</h3>
                          <p>{{ this.seo.description }}</p>
                        </a><div class="worksWith"><a href="{{ this.url }}/">
                         
                                {{#each this.workswith}}
                                <a href="/addons/workswith/fix-me/">
                                    <img src="/addons{{ this.icon }}" alt="{{ this.name }}" data-toggle="tooltip" title="{{ this.name }}">
                                </a>
																{{/each}}
                            
                        </div>
                    </div>
                  </div>

                {{/compare}}
              {{/each}}
              
            </div>
          

          

          
          <!-- end tiles -->
          
      </div> <!-- /tiles -->
     
          
                  <div class="pagination pagination-centered">
                      <ul>
                          
                              <li class="disabled"><span>«</span></li>
                          

                          
                              
                                  <li class="disabled"><span>1</span></li>
                              
                          
                              
                                  <li class="active"><a href="/addons/page2.html">2</a></li>
                              
                          
                              
                                  <li class="active"><a href="/addons/page3.html">3</a></li>
                              
                          
                              
                                  <li class="active"><a href="/addons/page4.html">4</a></li>
                              
                          
                              
                                  <li class="active"><a href="/addons/page5.html">5</a></li>
                              
                          

                          
                              <li class="active"><a href="/addons/page2.html"><span>»</span></a></li>
                          
                      </ul>
                  </div>
          
     
