        <script type="text/javascript">function addClickToBody(){
	$(document.body).bind('click', function(event) {
			if(disableClick==false){
				//window.alert("click on body");
				$('#signInOptions').toggle();
				urchinTracker('/MYOB2/Sign-In/Close');
				$(document.body).unbind('click');
				window.setTimeout(addClickToSignIn, 50);
			}
			disableClick = false;
	});
}
function addClickToSignIn(){
	$('.signIn').bind('click', function(event) {
		event.preventDefault();
		$('#signInOptions').toggle();
		urchinTracker('/MYOB2/Sign-In/Open');
		$('.signIn').unbind('click');
		window.setTimeout(addClickToBody, 50);
		window.setTimeout(disableClickOnMenu, 50);
	});
}
function disableClickOnMenu(){ 
	// to allow people to click the drop down and not hide the menu
	$('#signInOptions').bind('click', function(event) {
		disableClick = true;						
	});
}
//the onclick needs to be bound to the sign-in  - can be placed in another $(document).ready(function(){ if possible
var disableClick = false;
$(document).ready(function(){
	addClickToSignIn();
});
